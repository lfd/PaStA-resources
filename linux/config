[PaStA]
### Project name. Should not contain whitespaces ###
PROJECT_NAME = "Linux"
MODE = "mbox"

### necessary for upstream comparison. Patches within this range will be considered ###
UPSTREAM = "origin/master"

### Blacklist of upstream commit hashes to ignore for upstream comparison ###
UPSTREAM_BLACKLIST = "linux-blacklist"

### Project specific autothresholds for interactive rating ###
INTERACTIVE_THRESHOLD = 1.1
AUTOACCEPT_THRESHOLD = 0.82
DIFF_LINES_RATIO = 0.4
MESSAGE_DIFF_WEIGHT = 0.3

### Used for parsing mailinglists ###
[mbox]
MINDATE = 1970-01-01
MAXDATE = 2030-01-01

[mbox.patchwork]
# api url of a patchwork instance. Use a specific api version in order to
# prevent undefined behaviour when api changes.
# url = 'http://patchwork:8000/api/1.2/'

# Provide an api_token token or username/password if restricted api access is
# needed (e.g updating relations)
# token = 'your_token'
# username = 'your_username'
# password = 'your_password'

[mbox.raw]
# listname = "filename"
# ...
# Note: When using patchwork, pulled mails will be placed into lists with
# corresponding list-id. Therefore listname should be a list-id:
# 'linux-kernel.vger.kernel.org' = "lkml.mbox"

[mbox.pubin]
# Mailing lists are hosted somewhere. A ML host may host multiple lists.
# To define lists of a host, those lists need to be defined within the list of
# lists of a ML host:
#
# [mbox.pubin."some.ml.host"]
# lists = ["listname1", "listname2", ...]
#
# Shards (0.git, 1.git, ...) of public inboxes MUST be located under
# resources/project_name/resources/mbox/pubin/some.ml.host/listname1/ . PaStA
# will then try to automatically find shards beginning from 0.

